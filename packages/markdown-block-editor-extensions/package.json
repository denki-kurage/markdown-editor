		"@wordpress/scripts": "^30.24.0"
	},
	"dependencies": {
		"@kurage/markdown-core": "workspace:^",
		"@kurage/markdown-core-extensions": "workspace:^",
		"@kurage/markdown-block-editor": "workspace:^",
		"@svgr/webpack": "^8.1.0",
		"@wordpress/components": "^30.6.0",
		"@wordpress/element": "^6.37.0",
		"@wordpress/hooks": "^4.32.0",
		"@wordpress/i18n": "^6.10.0",
		"pkg": "^5.8.1",
		"react": "^19.2.3",
		"url-loader": "^4.1.1"
	}
